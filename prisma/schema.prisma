generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:secret@localhost:5432/openmusic?schema=public"
}

model albums {
  id    String  @id(map: "album_pkey") @db.VarChar(255)
  name  String  @db.VarChar(255)
  year  Int
  songs songs[]
}

model songs {
  id        String  @id @db.VarChar(255)
  title     String  @db.VarChar(255)
  year      Int
  performer String  @db.VarChar(255)
  genre     String  @db.VarChar(255)
  duration  Int?
  albumId   String? @db.VarChar(255)
  albums    albums? @relation(fields: [albumId], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "songs_album_id_fkey")
}
